<table id="example2" class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>Nama</th>
    <th>Nama Bank</th>
    <th>Nomor Rekening</th>
  </tr>
  </thead>
  <tbody>
    <% for (let i = 0; i < bank.length; i++) { %>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= bank[i].name %></td>
        <td><%= bank[i].bankName %></td>
        <td><%= bank[i].noRekening %></td>
        <td>
          <form action="/api/admin/bank/delete/<%= bank[i]._id %>?_method=DELETE" method="post">
            <a href="/admin/bank/edit/<%= bank[i]._id %>" class="btn btn-info btn-sm"><i class="fas fa-pencil-alt"></i> Edit</a>
            <button class="btn btn-danger btn-sm"><i class="fas fa-trash"></i>Hapus</button>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>