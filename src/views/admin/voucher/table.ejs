<table id="example2" class="table table-bordered table-hover">
  <thead>
  <tr>
    <th>No.</th>
    <th>Nama game</th>
    <th>Kategori</th>
    <th>Nominal</th>
    <th>Status</th>
    <th>Aksi</th>
  </tr>
  </thead>
  <tbody>
    <% for(let i = 0; i < voucher.length; i++){ %>
      <tr>
        <td><%= i + 1 %></td>
        <td><%= voucher[i].name %></td>
        <td><%= voucher[i]?.category?.name ?? '-' %></td>
        <td>
          <% voucher[i].nominal.forEach(nominal => { %>
            <span class="badge badge-warning">
              <%= nominal.coinName %> <%= nominal.coinQuantity %>
            </span>
          <% }) %>
        </td>
        <td>
          <% if (voucher[i].status === 'active') { %>
            <span class="badge badge-success">
              Aktif
            </span>
          <% } else { %>
            <span class="badge badge-danger">
              Tidak aktif
            </span>
          <% } %>
        </td>
        <td>
          <form action="/api/admin/voucher/delete/<%= voucher[i]._id %>?_method=DELETE" method="POST">
            <button class="btn btn-primary btn-sm">
              <i class=" 	fas fa-power-off"></i>
              <% if (voucher[i].status === 'active') { %>
                Matikan
              <% } else { %>
                Hidupkan
              <% } %>
            </button>
            <a href="/admin/voucher/edit/<%= voucher[i]._id %>" class="btn btn-info btn-sm">
              <i class="fas fa-pencil-alt"></i>
              Edit
            </a>
            <button class="btn btn-danger btn-sm">
              <i class="fas fa-trash"></i>
              Hapus
            </button>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>